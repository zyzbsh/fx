<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="inside_border_width" format="dimension" />
        <attr name="inside_border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallSpinFadeLoader" value="22" />
        </attr>
        <attr name="indicator_color" format="color" />
    </declare-styleable>

    <declare-styleable name="SwipeMenuLayout">
        <attr name="leftViewId" format="reference" />
        <attr name="rightViewId" format="reference" />
        <attr name="contentViewId" format="reference" />
    </declare-styleable>

    <declare-styleable name="RecyclerViewPager">
        <attr name="rvp_triggerOffset" format="float" />
        <attr name="rvp_flingFactor" format="float" />
        <attr name="rvp_singlePageFling" format="boolean" />
    </declare-styleable>

    <declare-styleable name="IndentifyingCode">
        <attr name="codeCount" format="integer|reference"></attr>
        <attr name="textSize" format="dimension"></attr>
    </declare-styleable>

</resources>